{
    "rewrites": [
      {
        "source": "/maintenance",
        "destination": "/contact"
      }
    ],
    "regions": ["all"],
    "functions": {
      "api/**.ts": {
        "memory": 128,
        "maxDuration": 10
      }
    },
    "crons": [
      {
        "path": "/api/reminder",
        "schedule": "0 9 * * 1"
      }
    ],
    "buildCommand": "npm run build"
  }
  